[
  {
    "outputFileName": "wwwroot/js/app.js",
    "inputFiles": [
      "wwwroot/app/api/*.js",
      "wwwroot/app/common/*.js",
      "wwwroot/app/home/*.js",
      "wwwroot/app/admin/*.js",
      "wwwroot/app/appointment/*.js",
      "wwwroot/app/login/*.js",
      "wwwroot/app/navbar/*.js",
      "wwwroot/app/app.module.js"
    ],
    "minify": {
      "enabled": true,
      "renameLocals": false
    }
  },
